class FourCal:
    def __init__(self, first, second):
        self.first = first
        self.second = second

    def setdata(self, first, second):
        self.first = first
        self.second = second

    def add(self):
        result = self.first + self.second
        return result

    def minus(self):
        result = self.first - self.second
        return result

    def multiply(self):
        result = self.first * self.second
        return result

    def divide(self):
        result = self.first / self.second
        return result

a = FourCal(4,2);

print(a.add())
print(a.minus())
print(a.multiply())
print(a.divide())

# class 안의 method를 사용한다.
# __init__은 예약어라고. 설계도에서 맨처음에 init이 실행됨
# class 변수는 픽스시켜 지정할수있음.
